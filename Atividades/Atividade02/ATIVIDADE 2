NUMERO PAR OU IMPAR

#include <stdio.h>
int main (void)
{
int num;
printf(" Digite o numero interiro: ");
scanf("%d",&num);
if(num%2==0)//operador %
{
printf("Par");
}
else
printf("Impar");
}

VALOR QUANTITATIVO

#include <stdio.h>

int main(void)
{
    int nota; /* nota*/
    printf ("Coloque uma certa nota de 1 a 5: ");
    scanf("%d", &nota);

    if (nota == 1)
    { 
    printf("Ruim");
    } 

    else if (nota == 2)
    {
    printf("Insuficiente");
    }

    else if (nota == 3)
    {
    printf("Suficiente");
    }

    else if (nota == 4)
    {
    printf("Bom");
    }

    else if (nota == 5)
    {
    printf("Ã“timo");
    }
 
}
